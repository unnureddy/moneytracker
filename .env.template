# AWS Configuration Environment Variables
# Copy this file to .env and fill in your AWS values after setting up AWS services

# AWS Region
REACT_APP_AWS_REGION=us-east-1

# Cognito User Pool Configuration
REACT_APP_USER_POOL_ID=your-user-pool-id
REACT_APP_USER_POOL_WEB_CLIENT_ID=your-web-client-id

# API Gateway Configuration
REACT_APP_API_ENDPOINT=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod

# Feature Flags
REACT_APP_ENABLE_AWS=false
